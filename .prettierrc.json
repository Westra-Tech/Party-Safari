{
	"arrowParens": "avoid",
	"bracketSameLine": true,
	"embeddedLanguageFormatting": "auto",
	"endOfLine": "lf",
	"jsxSingleQuote": false,
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"plugins": [
		"prettier-plugin-svelte"
	],
	"semi": true,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "none",
	"useTabs": true
}